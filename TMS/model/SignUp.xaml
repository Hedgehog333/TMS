<Window x:Class="TMS.model.SignUp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SignUp" Height="413" Width="444" ResizeMode="NoResize">
    <StackPanel Margin="20 0" >
        <Label Content="First Name" />
        <TextBox Height="27" TextWrapping="Wrap" Name="FName"/>
        <Label Content="Last Name"/>
        <TextBox Height="27" TextWrapping="Wrap" Name="LName"/>
        <Label Content="Second Name"/>
        <TextBox Height="27" TextWrapping="Wrap" Name="SName"/>
        <Label Content="Email"/>
        <TextBox Height="27" TextWrapping="Wrap" Name="Email"/>
        <Label Content="Password"/>
        <PasswordBox Height="27" Name="Password"/>
        <Label Content="Repeat password"/>
        <PasswordBox Height="27" Name="RepeatPassword" />
        <Button HorizontalAlignment="Right" Content="Sing Up" Height="35" Cursor="Hand" Width="109" Margin="0,10,0,0" Click="SignUP"/>
    </StackPanel>
</Window>
